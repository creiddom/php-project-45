#!/usr/bin/env php
<?php

$autoloaders = [
    __DIR__ . '/../vendor/autoload.php',
    __DIR__ . '/../../autoload.php',
];
foreach ($autoloaders as $file) {
    if (file_exists($file)) { require_once $file; break; }
}

use function Hexlet\Code\Engine\runGame;
use function Hexlet\Code\Games\BrainGames\generateRoundData;
use const Hexlet\Code\Games\BrainGames\DESCRIPTION;

runGame('Hexlet\\Code\\Games\\BrainGames\\generateRoundData', DESCRIPTION);
